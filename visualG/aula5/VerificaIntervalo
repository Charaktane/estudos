var
    numero: inteiro
inicio
    escreva("Digite um número: ")
    leia(numero)
    se (numero >= 100) e (numero <= 200) entao
        escreva("O número ", numero, " está no intervalo entre 100 e 200.")
    senao
        escreva("O número ", numero, " não está no intervalo entre 100 e 200.")
    fimse
fimalgoritmo
