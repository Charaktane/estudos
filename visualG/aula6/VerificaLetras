var
    letra1, letra2: caractere
inicio
    // Entrada de dados
    escreva("Digite a primeira letra: ")
    leia(letra1)
    
    escreva("Digite a segunda letra: ")
    leia(letra2)
    
    // Verificação das letras inseridas
    se (letra1 = 'A') e (letra2 = 'B') entao
        escreva("Letras inseridas corretamente!")
    senao
        escreva("Erro: As letras devem ser 'A' e 'B' nessa ordem.")
    fimse
fimalgoritmo
